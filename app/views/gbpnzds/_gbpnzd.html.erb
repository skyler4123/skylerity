<div id="<%= dom_id gbpnzd %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Epoch:</strong>
    <%= gbpnzd.created_at %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Quote:</strong>
    <%= gbpnzd.quote %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this gbpnzd", gbpnzd, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this gbpnzd', edit_gbpnzd_path(gbpnzd), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
